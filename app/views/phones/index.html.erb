<p id="notice"><%= notice %></p>
<div class="container-fluid">
  <h1>Phones</h1>
  <%= button_to 'Export', export_phones_path, method: 'get' %>
  <%= form_tag uploads_phones_path, :multipart => true do %>
   <label for="file">Upload text File</label> <%= file_field_tag "file" %>
   <%= submit_tag %>
<% end %>
  <table>
    <thead>
      <tr>
        <th>Phone</th>
        <th>Name</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody class="all_phone">
      <%= render partial: 'form', locals: {phone: @phone} %>
      <% @phones.each do |phone| %>
        <%= render partial: 'phone', locals: {phone: phone} %>
      <% end %>
    </tbody>
  </table>

  <br>

  <%#= link_to 'New Phone', new_phone_path, class: 'new_phone' %>
</div>

<%= javascript_pack_tag 'phones' %>
